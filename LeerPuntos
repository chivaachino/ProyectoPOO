import java.awt.Point;

import javax.swing.JOptionPane;

public class SimuladorDePuntos {
	int n;

	public SimuladorDePuntos(){
		//punto1.setLocation(4,8);
		//System.out.println(punto1.getX());	
		Point[] arreglo = new Point[n];
		for (int i = 0; i < arreglo.length; i++) {
			arreglo[i]=new Point(1*i,2*i/2);
		}
	
	}

	public static void main(String[] args) {
		SimuladorDePuntos sp = new SimuladorDePuntos();
		
	}
}
